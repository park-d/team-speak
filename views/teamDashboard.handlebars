<div class="grid-container">
    <div class="grid-x grid-margin-x small-up-1 medium-up-2 large-up-3">
        <div class="cell">
            {{#each posts}}
            <div class="card custom-card">
                <div class="card-section">
                    <h4>{{title}}</h4>
                    <p>{{body}}
                    </p>
                    <p>{{username}} - {{updated_at}}</p>
                    <a href="/comments">Click to show Comments</a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>

